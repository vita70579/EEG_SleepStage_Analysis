    "def smoothingIV(data, pred, score):\n",
    "    \n",
    "    # 測試集經過非平穩平滑後,被預測為REM的數據標籤\n",
    "    rem_idx = np.where(pred == 3)\n",
    "    \n",
    "    sc = score[rem_idx][:,0]\n",
    "    std = np.std(sc)\n",
    "    u = np.mean(sc)\n",
    "    \n",
    "    template = pred[rem_idx]\n",
    "    template[np.where((sc>=u + 1.2*std))] = 0\n",
    "    \n",
    "    j = 0\n",
    "    for i in range(len(pred)):\n",
    "        if (pred[i] == 3):\n",
    "            pred[i] = template[j]\n",
    "            j += 1\n",
    "        else:\n",
    "            pass\n",
    "\n",
    "    return pred"
