    "def smoothingIII(data, thr=0.5, winlength=5):\n",
    "\n",
    "    half_win = winlength // 2\n",
    "    for pointer in range(half_win, len(data) - half_win):\n",
    "        mode = stats.mode(data[(pointer - half_win):(pointer + half_win + 1)])\n",
    "        if ((data[pointer] != mode[0][0]) and (mode[1][0] >= thr * winlength)):\n",
    "            data[pointer] = mode[0][0]\n",
    "\n",
    "    return data"
